target=multirun.adaptive.minimax.SimplePasswordCheck

classpath=${spf-sca}/build/examples;${spf-sca}/build/classes
sourcepath=${spf-sca}/src/examples

mutirun.secure_method=multirun.adaptive.minimax.SimplePasswordCheck.check

#listener=sidechannel.multirun.adaptive.OptimizedAdaptiveAttackListener

adaptive=minmax

multirun.num_run = 12
sidechannel.high_input_size = 4

symbolic.min_byte=0
symbolic.max_byte=3

symbolic.min_int=0
symbolic.max_int=3

sidechannel.min_high=0
sidechannel.max_high=3

sidechannel.tmpDir = ${spf-sca}/build/tmp
symbolic.reliability.tmpDir = ${spf-sca}/build/tmp/mc
symbolic.reliability.omegaPath=${spf-sca}/tools/omega/oc
symbolic.reliability.lattePath=${spf-sca}/tools/latte-integrale-1.7.3/bin/count

symbolic.debug = false

sidechannel.verbose = false

#symbolic.dp=z3
#symbolic.dp=no_solver

